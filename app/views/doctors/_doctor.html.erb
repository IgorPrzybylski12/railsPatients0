<div id="<%= dom_id doctor %>">
  <p>
    <strong>First name:</strong>
    <%= doctor.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= doctor.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= doctor.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= doctor.phone %>
  </p>

  <p>
    <strong>Specialization:</strong>
    <%= doctor.specialization %>
  </p>

</div>
